@model IEnumerable<AbleProject_Empty.Models.ResumeModel>

@{
    ViewBag.Title = "ShowResume";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Resumes</h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table">
    <tr>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.StudentId)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.StudentName)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.Contact)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.School)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.PlusTwoPercentage)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.College)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.Branch)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.GraduationPercentage)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.Backlogs)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th style="background-color:aquamarine">
            @Html.DisplayNameFor(model => model.PostedOn)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StudentId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.School)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PlusTwoPercentage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.College)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Branch)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GraduationPercentage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Backlogs)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PostedOn)
        </td>
